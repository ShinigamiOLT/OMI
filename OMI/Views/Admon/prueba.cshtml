@model IEnumerable<OIM_DAL.Sp_AllPedidoXEstatusXAdmin_Result>
<div class="row">
<div class="col-md-12">
    <div class="panel panel-border panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">Ingenieria Aplicada OnePro </h3>
        </div>
        <div class="panel-body">
            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table id="assets-data-table" class="table table-striped table-bordered" style="width: 100%">
                <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Folio)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Categoria)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Descripcion)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Unidad)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Cantidad)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Autoriza)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Surtido)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Formato)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NombreFormato)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Usuario)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Area)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Fecha)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Estatus)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Infra)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FAutorizacion)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Proveedor)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Nombre)
                    </th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Folio)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Categoria)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Descripcion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Unidad)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Cantidad)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Autoriza)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Surtido)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Formato)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NombreFormato)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Usuario)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Area)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Fecha)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Estatus)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Infra)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FAutorizacion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Proveedor)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Nombre)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                            @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                            @Html.ActionLink("Delete", "Delete", new {id = item.Id})
                        </td>
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>



